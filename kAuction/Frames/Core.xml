		<Anchors>
			<Anchor point="TOP" relativeTo="$parent" relativePoint="TOP" />
		</Anchors>
		<Anchors>
			<Anchor point="BOTTOM" relativeTo="$parent" relativePoint="BOTTOM" />
		</Anchors>
