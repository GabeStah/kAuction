							</OnEnter>							
						</Scripts>
						<NormalTexture file="Interface\AddOns\kAuction\Images\Textures\medal2-grey" />
						<HighlightTexture file="Interface\AddOns\kAuction\Images\Textures\medal2" />
					</Button>				
