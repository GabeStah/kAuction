<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\UI.xsd" xmlns="http://www.blizzard.com/wow/ui/">
	<Frame name="kAuctionBidWidget" virtual="true" enableMouse="true" hidden="true">
		<Size>
			<AbsDimension y="24" />
		</Size>
		<Layers>
			<Layer level="HIGHLIGHT">
				<Texture inherits="kAuctionHighlightTextureTemplate"/>
			</Layer>
		</Layers>
		<Anchors>
			<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT">
				<Offset>
					<AbsDimension x="5" y="0" />
				</Offset>
			</Anchor>
			<Anchor point="RIGHT" relativeTo="$parent" relativePoint="RIGHT">
				<Offset>
					<AbsDimension x="-22" y="0" />
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
			<Button name="$parentCurrentItem" inherits="kAuctionIconTemplate" id="1">
				<Anchors>
					<Anchor point="LEFT" />
				</Anchors>
			</Button>
			<Frame name="$parentName">
				<Size>
					<AbsDimension x="80" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentCurrentItem" relativePoint="RIGHT"/>
				</Anchors>
				<Layers>
					<Layer level="BORDER">
						<FontString name="$parentText" font="Fonts\FRIZQT__.TTF" text="Name" justifyH="LEFT">
							<Anchors>
								<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
								<Anchor point="RIGHT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
							</Anchors>
							<FontHeight>
								<AbsValue val="9" />
							</FontHeight>
							<Color r="0.9" g="0.82" b="0" />
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						kAuction:OnBidNameOnEnter(self);
					</OnEnter>
					<OnLeave>
						local tip = kAuction.qTip:Acquire("GameTooltip");
						tip:Hide();
					</OnLeave>
				</Scripts>
			</Frame>
			<Frame name="$parentRoll">
				<Size>
					<AbsDimension x="50" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentName" relativePoint="RIGHT"/>
				</Anchors>
				<Layers>
					<Layer level="BORDER">
						<FontString name="$parentText" font="Fonts\FRIZQT__.TTF" text="Roll" justifyH="LEFT">
							<Anchors>
								<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
								<Anchor point="RIGHT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
							</Anchors>
							<FontHeight>
								<AbsValue val="9" />
							</FontHeight>
							<Color r="0.9" g="0.82" b="0" />
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						kAuction:Gui_CreateTooltipBidRollOnEnter(self)
					</OnEnter>
					<OnLeave>
						kAuction:Gui_OnBidItemsWonLeave(self)
					</OnLeave>
				</Scripts>
			</Frame>
			<Frame name="$parentItemsWon">
				<Size>
					<AbsDimension x="60" y="24"/>
				</Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentRoll" relativePoint="RIGHT" />
					<Anchor point="RIGHT" relativeTo="$parentVote" relativePoint="LEFT" />
				</Anchors>
				<Layers>
					<Layer level="BORDER">
						<FontString name="$parentText" font="Fonts\FRIZQT__.TTF" text="#/#/#" justifyH="LEFT">
							<Anchors>
								<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
								<Anchor point="RIGHT">
									<Offset>
										<AbsDimension x="0" y="0"/>
									</Offset>
								</Anchor>
							</Anchors>
							<FontHeight>
								<AbsValue val="9" />
							</FontHeight>
							<Color r="0.9" g="0.82" b="0" />
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Frame name="$parentSelectFrame" enableMouse="true">
						<Size>
							<AbsDimension x="40" y="35"/>
						</Size>
						<Anchors>
							<Anchor point="BOTTOM" relativeTo="$parent" relativePoint="TOP">
								<Offset x="-2" />
							</Anchor>
						</Anchors>
						<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
							<BackgroundInsets>
								<AbsInset left="4" right="4" top="4" bottom="4" />
							</BackgroundInsets>
							<TileSize>
								<AbsValue val="16" />
							</TileSize>
							<EdgeSize>
								<AbsValue val="16" />
							</EdgeSize>
							<BorderColor>
								<Color r=".3" g=".3" b=".3"/>
							</BorderColor>
						</Backdrop>
					</Frame>
				</Frames>
				<Scripts>
					<OnEnter>
						kAuction:OnBidItemsWonEnter(self)
					</OnEnter>
					<OnLeave>
						kAuction:Gui_OnBidRollOnLeave(self)
					</OnLeave>
				</Scripts>
			</Frame>
			<Button name="$parentVote" inherits="OptionsButtonTemplate" text="Vote">
				<Size>
					<AbsDimension x="30" y="16" />
				</Size>
				<Anchors>
					<Anchor point="RIGHT">
						<Offset>
							<AbsDimension x="-3" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
		</Frames>
		<Scripts>
			<OnEnter>
				<!--kAuction:Gui_AuctionItemNameOnEnter(self)-->
			</OnEnter>
			<OnLeave>
				kAuctionTooltip:Hide()
			</OnLeave>
		</Scripts>
	</Frame>
</Ui>